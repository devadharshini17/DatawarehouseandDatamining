age=c(13, 15, 16, 16, 19, 20, 20, 21, 22, 22, 25, 25, 25, 25, 30, 33, 33, 35, 35, 35, 35, 36, 40, 45, 46, 52, 70.)
r=mean(age)
r
s=median(age)
s
getmode = function(v){uniqv = unique(v)
+ uniqv[which.max(tabulate(match(v,uniqv)))]}
result = getmode(age)
print(result)
midrange=min(age)+max(age)/2
midrange
Q1=quantile(age,.25)
